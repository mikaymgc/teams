<div class="team-name-alter">
  <%= form_with model: @team, local: true do |f| %>
  <div class="teams">
      <%= f.text_field :team_name, class:"form-control", id:"team-name", placeholder:"チーム名（必須 40文字まで)", maxlength:"40" %>
      <%= f.submit "変更する", class:"btn btn-primary", id:"team-submit-btn" %>
  </div>
  <% end %>
</div>