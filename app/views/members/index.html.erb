<p>★メンバー 一覧★</p>

<div class="member row">
  <ul class='member-lists'>
    <% @members.each do |member| %>
      <li class='list'>
        <span>
          <%= link_to team_path(member.id) do %>
            <%= member.name %>
          <% end %>
        </span>
      </li>
    <% end %>
  </ul>
</div>